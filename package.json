{
  "name": "api-ts-node-jest",
  "version": "1.0.0",
  "description": "Typescript Node.js API",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:local": "ts-node-dev ./src/index.ts",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "test:functional": "jest --projects ./test --forceExit --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nathan-oliveira/Treinamento_Nodejs_TypeScript_TDD.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "Nathan Oliveira",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nathan-oliveira/Treinamento_Nodejs_TypeScript_TDD/issues"
  },
  "homepage": "https://github.com/nathan-oliveira/Treinamento_Nodejs_TypeScript_TDD#readme",
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^14.0.27",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "eslint": "^7.7.0",
    "jest": "^26.4.0",
    "ts-jest": "^26.2.0",
    "ts-node-dev": "^1.0.0-pre.57",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "module-alias": "^2.2.2"
  }
}
